{
    "name": "omnipay/common",
    "type": "library",
    "description": "Common components for Omnipay payment processing library",
    "keywords": [
        "league",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-common",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-common/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "League\\Omnipay\\Common\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Omnipay\\Common\\" : "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "alcohol/iso4217": "^3.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "omnipay/tests": "^3.0",
        "guzzlehttp/guzzle": "^6.2.1",
        "zendframework/zend-diactoros": "^1.1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
